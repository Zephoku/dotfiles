$(document).on('ready', function() {

  $window.scroll(function() {
    var currScroll = s.getScrollTop();
    playElementAtBounds('audio8', currScroll, 5000, 6000)
    playElementAtBounds('audio14', currScroll, 9000, 10000);
    playElementAtBounds('video15', currScroll, 10000, 11000);
    playElementAtBounds('audio16', currScroll, 11000, 12000);
    playElementAtBounds('video19', currScroll, 14000, 15000);
    playElementAtBounds('audio22', currScroll, 17000, 18000);
    playElementAtBounds('audio23', currScroll, 18000, 19000);
    playElementAtBounds('video24', currScroll, 19000, 20000);
    playElementAtBounds('audio27', currScroll, 21000, 22000);
  });

});
