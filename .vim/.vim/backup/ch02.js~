$(document).on('ready', function() {

  $window.scroll(function() {
    var currScroll = s.getScrollTop();
    playElementAtBounds('audio3', currScroll, 1300, 2300)
    playElementAtBounds('video4', currScroll, 2300, 3300);
    playElementAtBounds('audio5', currScroll, 3000, 4000);
    playElementAtBounds('audio7', currScroll, 4000, 5000);
    playElementAtBounds('video8', currScroll, 5000, 6000);
    playElementAtBounds('audio9', currScroll, 6000, 7000);
    playElementAtBounds('video10', currScroll, 7000, 8000);
    playElementAtBounds('audio12', currScroll, 8000, 9000);
    playElementAtBounds('video13', currScroll, 9000, 10000);
    playElementAtBounds('audio14', currScroll, 10000, 11000);
    playElementAtBounds('audio16', currScroll, 11000, 12000);
  });

});
