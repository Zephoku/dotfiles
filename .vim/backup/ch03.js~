$(document).on('ready', function() {

  $window.scroll(function() {
    var currScroll = s.getScrollTop();
    playElementAtBounds('audio2', currScroll, 1000, 2000)
    playElementAtBounds('video4', currScroll, 3000, 4000);
    playElementAtBounds('video5', currScroll, 4000, 5000);
    playElementAtBounds('audio6', currScroll, 5000, 6000);
    playElementAtBounds('video7', currScroll, 6000, 7000);
    playElementAtBounds('video8', currScroll, 7000, 8000);
    playElementAtBounds('audio9', currScroll, 8000, 9000);
    playElementAtBounds('audio10', currScroll, 9000, 10000);
    playElementAtBounds('video11', currScroll, 10000, 11000);
    playElementAtBounds('audio12', currScroll, 11000, 12000);
    playElementAtBounds('audio14', currScroll, 12000, 13000);
    playElementAtBounds('video16', currScroll, 14000, 15000);
    playElementAtBounds('audio16', currScroll, 14000, 15000);
    playElementAtBounds('audio17', currScroll, 15000, 16000);
    playElementAtBounds('video19', currScroll, 17000, 18000);
    playElementAtBounds('video20', currScroll, 18000, 19000);
    playElementAtBounds('audio20', currScroll, 18000, 19000);
    playElementAtBounds('audio22', currScroll, 19000, 20000);
    playElementAtBounds('audio24', currScroll, 21000, 22000);
    playElementAtBounds('video25', currScroll, 22000, 23000);
    playElementAtBounds('audio26', currScroll, 23000, 24000);
    playElementAtBounds('video27', currScroll, 24000, 25000);
    playElementAtBounds('video28', currScroll, 25000, 26000);
    playElementAtBounds('audio28', currScroll, 25000, 26000);
    playElementAtBounds('video29', currScroll, 26000, 27000);
    playElementAtBounds('audio29', currScroll, 26000, 27000);
    playElementAtBounds('video30', currScroll, 27000, 28000);
    playElementAtBounds('audio30', currScroll, 27000, 28000);
  });

});
